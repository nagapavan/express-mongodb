{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "watch": [
        "src"
    ],
    "exec": "ts-node-dev --inspect=0.0.0.0:9222 -r tsconfig-paths/register src/index",
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts"
}